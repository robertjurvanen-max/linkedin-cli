{
  "name": "linkedin-cli",
  "version": "1.0.0",
  "description": "A comprehensive CLI for interacting with the LinkedIn API with OAuth 2.0",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "linkedin": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/**/*.ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "linkedin",
    "cli",
    "api",
    "oauth",
    "social-media"
  ],
  "author": "Robert Jurvanen",
  "license": "MIT",
  "dependencies": {
    "commander": "^12.1.0",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0",
    "vitest": "^2.1.0"
  }
}
